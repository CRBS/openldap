<% if @uri %>
    URI <%= @uri.join(' ') %>
<% end %>
<% if @base %>
    BASE <%= @base %>
<% end %>
<% if @tls_cacert %>
    TLS_CACERT <%= @tls_cacert %>
<% end %>
<% if @tls_reqcert %>
    TLS_REQCERT <%= @tls_reqcert %>
<% end %>

